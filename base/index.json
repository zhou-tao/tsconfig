{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "allowJs": false,
    "outDir": "dist",
    "jsx": "preserve",
    "noUnusedLocals": true,
    "removeComments": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
